  "sourceRoot": "libs/constants/shared/src",
  "projectType": "library",
  "tags": ["scope:shared type:constants"],
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
