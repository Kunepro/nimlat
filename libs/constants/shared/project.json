        "outputPath": "dist/libs/constants/shared",
        "main": "libs/constants/shared/src/index.ts",
        "tsConfig": "libs/constants/shared/tsconfig.lib.json",
        "assets": ["libs/constants/shared/*.md"],
        "format": ["cjs"],
        "generatePackageJson": true
      }
    }
  }
}
